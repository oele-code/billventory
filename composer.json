{
    "name": "oele-code/billventory",
    "description": "Inventory & Billing Manager",
    "keywords": [
        "Inventory",
        "Billing"
    ],
    "license": "propietary",
    "author": {
        "name": "Osmell Caicedo",
        "email": "correo.oele@gmail.com"
    },
    "type": "project",
    "require": {
        "php": "^7.1",
        "laravel/framework": "5.3.*",
        "laravelcollective/html": "5.3.*",
        "barryvdh/laravel-dompdf": "^0.8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "rector/rector": "^0.12.19"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "valet php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "valet php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "valet php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
